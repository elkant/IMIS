<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>loadMOH731</servlet-name>
        <servlet-class>moh731.loadMOH731</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadFacilities</servlet-name>
        <servlet-class>General.loadFacilities</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updatedbpword</servlet-name>
        <servlet-class>database.updatedbpword</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loadMOH731</servlet-name>
        <url-pattern>/loadMOH731</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadFacilities</servlet-name>
        <url-pattern>/loadFacilities</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updatedbpword</servlet-name>
        <url-pattern>/updatedbpword</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
